{
    "routes": [
        {
            "route": "/callback",
            "serve": "/callback.html",
            "statusCode": 200
        },
        {
            "route": "/account",
            "serve": "/account.html",
            "statusCode": 200
        },
        {
            "route": "/app",
            "serve": "/app.html",
            "statusCode": 200
        },
        {
            "route": "/settings",
            "serve": "/settings.html",
            "statusCode": 200
        },
        {
            "route": "/projects",
            "serve": "/projects.html",
            "statusCode": 200
        },
        {
            "route": "/projects/*",
            "serve": "/projects/[projectId].html",
            "statusCode": 200
        },
        {
            "route": "/",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/*",
            "serve": "/404.html",
            "statusCode": 200
        }
    ]
}